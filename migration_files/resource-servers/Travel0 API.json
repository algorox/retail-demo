{
  "name": "Travel0 API",
  "identifier": "https://legacy-demo-test-travel0.auth0.cloud/api",
  "allow_offline_access": false,
  "skip_consent_for_verifiable_first_party_clients": true,
  "token_lifetime": 86400,
  "token_lifetime_for_web": 7200,
  "signing_alg": "RS256",
  "scopes": [
    {
      "value": "read:payment",
      "description": "Read payment methods"
    },
    {
      "value": "update:payment",
      "description": "Update payment methods"
    },
    {
      "value": "approve:travel",
      "description": "Can approve travel for corporate users"
    }
  ],
  "token_dialect": "access_token_authz"
}
